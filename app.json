{
  "name": "Slack to Trello card creation",
  "description": "Enable Trello card creation from Slack",
  "repository": "https://github.com/Greenns/slack-to-trello/",
  "keywords": ["slack", "trello", "javascript", "heroku"],
  "env": {
    "TRELLO_KEY" : {
      "description": "trello key",
      "required": true
    },
    "TRELLO_TOKEN": {
      "description": "trello token",
      "required": true
    },
    "SLACK_HOOK_URL": {
      "description": "slack webhook url",
      "required": true
    },
    "SLACK_CHANNEL": {
      "description": "slack chanel",
      "required": true
    }
  }
}
